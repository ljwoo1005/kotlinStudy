<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/moveToolbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="툴바 이동"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/moveFragment"/>

    <Button
        android:id="@+id/moveFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프래그먼트 이동"
        app:layout_constraintTop_toBottomOf="@+id/moveToolbar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/moveFragmentBackStack"/>

    <Button
        android:id="@+id/moveFragmentBackStack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프래그먼트 백스택 이동"
        app:layout_constraintTop_toBottomOf="@+id/moveFragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>